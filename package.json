{
  "name": "nikuman",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "jaxx2104 <jaxx2104@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && npm run build-copy-ssr",
    "build-copy-ssr": "rimraf functions/ssr && mkdirp functions/ssr && cp -R .nuxt/dist functions/ssr/dist",
    "build-storybook": "build-storybook -c .storybook -o .tmp/storybook",
    "serve": "firebase serve",
    "deploy": "firebase deploy",
    "start": "nuxt start",
    "storybook": "start-storybook -s ./src/static -c .storybook -p 9001",
    "generate": "nuxt generate",
    "lint": "prettier ./**/*.{css,js,json,scss,vue}"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/pwa": "2.6.0",
    "grpc": "1.17.0",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "3.1.0",
    "@storybook/vue": "4.1.4",
    "babel-eslint": "8.2.6",
    "babel-preset-vue": "^2.0.2",
    "copy-to-clipboard": "3.0.8",
    "date-fns": "1.30.1",
    "dotenv": "6.2.0",
    "eslint": "5.4.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "4.7.1",
    "firebase": "5.7.2",
    "firebase-tools": "6.2.2",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "lodash": "4.17.11",
    "mkdirp": "0.5.1",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "vue-styled-components": "1.4.3",
    "vuexfire": "2.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
