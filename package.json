{
  "name": "nikuman",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "jaxx2104 <jaxx2104@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && npm run build-copy-ssr",
    "build-copy-ssr": "rimraf functions/ssr && mkdirp functions/ssr && cp -R .nuxt/dist functions/ssr/dist",
    "build-storybook": "build-storybook -c .storybook -o .tmp/storybook",
    "serve": "firebase serve",
    "deploy": "firebase deploy",
    "start": "nuxt start",
    "storybook": "start-storybook -s ./src/static -c .storybook -p 9001",
    "generate": "nuxt generate",
    "lint": "prettier ./**/*.{css,js,json,scss,vue}",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{css,js,json,scss,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/pwa": "2.0.8",
    "grpc": "1.14.1",
    "nuxt": "1.4.2"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "2.3.3",
    "@storybook/vue": "3.4.10",
    "babel-eslint": "8.2.6",
    "babel-plugin-root-import": "6.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "copy-to-clipboard": "3.0.8",
    "date-fns": "1.29.0",
    "dotenv": "6.0.0",
    "eslint": "5.4.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "4.7.1",
    "firebase": "5.4.1",
    "firebase-tools": "4.2.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "lodash": "4.17.10",
    "mkdirp": "0.5.1",
    "prettier": "1.14.2",
    "rimraf": "2.6.2",
    "storybook-addon-vue-info": "0.6.1",
    "vue-styled-components": "1.4.0",
    "vuexfire": "2.3.1"
  }
}
